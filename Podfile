use_frameworks!

target 'Hyperspace-iOS' do
  platform :ios, '8.0'

  target 'Hyperspace-iOSTests' do
    inherit! :search_paths

  end

end

target 'Hyperspace-tvOS' do
  platform :tvos, '9.0'

  target 'Hyperspace-tvOSTests' do
    inherit! :search_paths

  end

end

target 'Hyperspace-watchOS' do
  platform :watchos, '2.0'

end
